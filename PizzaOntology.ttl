<http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial> rdf:type owl:Ontology ;
                                                                       rdfs:comment "A tutorial ontology for the Pizza domain." .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#hasBase
:hasBase rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :hasIngredient ;
         owl:inverseOf :isBaseOf ;
         rdfs:domain :Pizza ;
         rdfs:range :PizzaBase .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#hasIngredient
:hasIngredient rdf:type owl:ObjectProperty ;
               owl:inverseOf :isIngredientOf .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#hasSpiciness
:hasSpiciness rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :PizzaTopping ;
              rdfs:range :Spiciness .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#hasTopping
:hasTopping rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasIngredient ;
            owl:inverseOf :isToppingOf ;
            rdfs:domain :Pizza ;
            rdfs:range :PizzaTopping .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#isBaseOf
:isBaseOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :isIngredientOf .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#isIngredientOf
:isIngredientOf rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#isMilderThan
:isMilderThan rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              owl:inverseOf :isSpicierThan ;
              rdf:type owl:TransitiveProperty .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#isSpicierThan
:isSpicierThan rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdf:type owl:TransitiveProperty ;
               rdfs:domain :Spiciness ;
               rdfs:range :Spiciness .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#isToppingOf
:isToppingOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :isIngredientOf .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#hasCaloricContent
:hasCaloricContent rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain :Pizza ;
                   rdfs:range xsd:integer .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#AmericanaHotPizza
:AmericanaHotPizza rdf:type owl:Class ;
                   rdfs:subClassOf :NamedPizza ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasTopping ;
                                     owl:someValuesFrom :JalapenoPepperTopping
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasTopping ;
                                     owl:someValuesFrom :MozzarellaTopping
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasTopping ;
                                     owl:someValuesFrom :PepperoniTopping
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasTopping ;
                                     owl:someValuesFrom :TomatoTopping
                                   ] ;
                   rdfs:comment "A pizza that only has Jalapeno, Mozzarella, Tomato, and Pepperoni toppings." .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#AmericanaPizza
:AmericanaPizza rdf:type owl:Class ;
                rdfs:subClassOf :NamedPizza ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasTopping ;
                                  owl:someValuesFrom :MozzarellaTopping
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasTopping ;
                                  owl:someValuesFrom :PepperoniTopping
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasTopping ;
                                  owl:someValuesFrom :TomatoTopping
                                ] ;
                rdfs:comment "A pizza that only has Mozzarella, Tomato, and Pepperoni toppings." .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#AnchovyTopping
:AnchovyTopping rdf:type owl:Class ;
                rdfs:subClassOf :SeafoodTopping .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#CaperTopping
:CaperTopping rdf:type owl:Class ;
              rdfs:subClassOf :VegetableTopping .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#CheeseTopping
:CheeseTopping rdf:type owl:Class ;
               rdfs:subClassOf :PizzaTopping .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#CheesyPizza
:CheesyPizza rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :Pizza
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasTopping ;
                                                          owl:someValuesFrom :CheeseTopping
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:comment "A Defined Class. If \"Equivalent To\" conditions are satisfied, then the individual belongs to CheesyPizza Defined Class. All other classes are Primitive Classes, where only necessary consitions are defined. When you have necessary and Sufficient conditions, you have a defined class." .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#DeepPanBase
:DeepPanBase rdf:type owl:Class ;
             rdfs:subClassOf :PizzaBase ;
             owl:disjointWith :ThinAndCrispyBase .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#GreenPepperTopping
:GreenPepperTopping rdf:type owl:Class ;
                    rdfs:subClassOf :PepperTopping .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#HamTopping
:HamTopping rdf:type owl:Class ;
            rdfs:subClassOf :MeatTopping .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#InterestingPizza
:InterestingPizza rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :Pizza
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasTopping ;
                                                               owl:minQualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                                                               owl:onClass :PizzaTopping
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#JalapenoPepperTopping
:JalapenoPepperTopping rdf:type owl:Class ;
                       rdfs:subClassOf :PepperTopping ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasSpiciness ;
                                         owl:hasValue :Hot
                                       ] .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#MargheritaPizza
:MargheritaPizza rdf:type owl:Class ;
                 rdfs:subClassOf :NamedPizza ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasTopping ;
                                   owl:someValuesFrom :MozzarellaTopping
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasTopping ;
                                   owl:someValuesFrom :TomatoTopping
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasTopping ;
                                   owl:allValuesFrom [ rdf:type owl:Class ;
                                                       owl:unionOf ( :MozzarellaTopping
                                                                     :TomatoTopping
                                                                   )
                                                     ]
                                 ] ;
                 rdfs:comment "A pizza that only has Mozzarella and Tomato toppings." .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#MeatTopping
:MeatTopping rdf:type owl:Class ;
             rdfs:subClassOf :PizzaTopping .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#MozzarellaTopping
:MozzarellaTopping rdf:type owl:Class ;
                   rdfs:subClassOf :CheeseTopping ;
                   owl:disjointWith :ParmesanTopping .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#MushroomTopping
:MushroomTopping rdf:type owl:Class ;
                 rdfs:subClassOf :VegetableTopping .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#NamedPizza
:NamedPizza rdf:type owl:Class ;
            rdfs:subClassOf :Pizza .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#OliveTopping
:OliveTopping rdf:type owl:Class ;
              rdfs:subClassOf :VegetableTopping .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#ParmesanTopping
:ParmesanTopping rdf:type owl:Class ;
                 rdfs:subClassOf :CheeseTopping .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#PepperTopping
:PepperTopping rdf:type owl:Class ;
               rdfs:subClassOf :VegetableTopping .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#PepperoniTopping
:PepperoniTopping rdf:type owl:Class ;
                  rdfs:subClassOf :MeatTopping .


###  http://www.semanticweb.org/akp61793/ontologies/2023/0/PizzaTutorial#Pizza
:Pizza rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :hasBase ;
                         owl:someValuesFrom :PizzaBase
                       ] ,
                       [ rdf:type owl:Restriction ;
